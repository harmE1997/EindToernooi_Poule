<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="EindToernooi_Poule.Views.isoFileBrowser">
	<UserControl.Resources>
		<Image x:Key="OpenFolderIcon" Source="/Assets/Icons/openfoldericon.png" />
	</UserControl.Resources>
	<StackPanel Orientation="Horizontal">
		<Button Name="btnBrowse" Grid.Column="1" VerticalContentAlignment="Center"
				HorizontalContentAlignment="Stretch" Click="Browse" Height="33" Width="95" VerticalAlignment="Stretch" BorderBrush="Black" BorderThickness="1">
			<StackPanel Orientation="Horizontal">
				<Image Source="/Assets/Icons/openfoldericon.png"/>
				<TextBlock Text=" Browse" VerticalAlignment="Center"/>
			</StackPanel>
		</Button>
		<TextBox Name="tbOutput" Grid.Column="0" Height="33" Width="230"/>
	</StackPanel>
</UserControl>
